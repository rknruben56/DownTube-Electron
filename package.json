{
  "name": "downtube",
  "version": "1.0.0",
  "description": "Electron App that downloads YouTube videos",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "pack": "electron-builder --dir",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64",
    "postinstall": "node src/youtube-dl/scripts/download.js"
  },
  "build": {
    "appId": "com.rknruben56.downtube",
    "productName": "DownTube",
    "extraResources": [
      "./extraResources/**"
    ],
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "author": "rknruben56",
  "license": "MIT",
  "dependencies": {
    "@rauschma/stringio": "^1.4.0",
    "bootstrap": "^4.4.1",
    "ffmpeg-static": "^4.0.1",
    "mkdirp": "^1.0.3",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.1.3",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.9.26",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "acorn": "^6.4.1",
    "ajv": "^6.12.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "electron": "^13.6.6",
    "electron-builder": "^22.4.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.2.3",
    "lodash": "^4.17.15",
    "rimraf": "^2.7.1",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rknruben56/DownTube"
  },
  "keywords": [
    "YouTube",
    "Electron",
    "React",
    "Typescript",
    "Webpack"
  ],
  "bugs": {
    "url": "https://github.com/rknruben56/DownTube/issues"
  },
  "homepage": "https://github.com/rknruben56/DownTube#readme"
}
